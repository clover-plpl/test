{
	"name": "test",
	"displayName": "test",
	"description": "test dayo!",
	"version": "0.4.0",
	"icon": "app.png",
	"publisher": "example-publisher",
	"engines": {
		"vscode": "^1.45.0"
	},
	"categories": [
		"Other"
	],
	"activationEvents": [
		"onCommand:extension.helloWorld",
		"onLanguage:markdown"
	],
	"main": "./out/extension.js",
	"contributes": {
		"commands": [
			{
				"command": "extension.helloWorld",
				"title": "Hello World",
				"icon": {
					"dark": "https://raw.githubusercontent.com/clover-plpl/test/e31e22be6da3c514c1974f06046a2201f740b638/w.svg",
					"light": "b.svg"
				}
			}
		],
		"menus": {
			"editor/title": [
				{
					"command": "extension.helloWorld",
					"group": "navigation"
				}
			]
		}
	},
	"scripts": {
		"vscode:prepublish": "npm run compile",
		"compile": "tsc -p ./",
		"watch": "tsc -watch -p ./",
		"pretest": "npm run compile",
		"test": "node ./out/test/runTest.js"
	},
	"devDependencies": {
		"@types/glob": "^7.1.1",
		"@types/mocha": "^5.2.7",
		"@types/node": "^12.11.7",
		"@types/vscode": "^1.45.0",
		"glob": "^7.1.5",
		"mocha": "^10.2.0",
		"typescript": "^3.6.4",
		"tslint": "^5.20.0",
		"vscode-test": "^1.2.2"
	}
}
